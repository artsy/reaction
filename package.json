{
  "name": "@artsy/reaction-force",
  "version": "0.1.47",
  "description": "Force’s React Components",
  "main": "dist/index.js",
  "repository": "https://github.com/artsy/reaction-force.git",
  "author": "Eloy Durán <eloy.de.enige@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "assets",
    "data",
    "docs"
  ],
  "scripts": {
    "start": "node verify-node-version.js && ts-babel-node index.js",
    "start:dist": "npm run compile && node index.js ./dist",
    "test": "node verify-node-version.js && jest",
    "storybook": "node verify-node-version.js && start-storybook -p 9001 -c .storybook -s ./assets",
    "deploy-storybook": "storybook-to-ghpages",
    "relay2ts": "relay2ts src/components/*.tsx --update",
    "sync-schema": "cd externals/metaphysics && git fetch && git checkout origin/master && yarn install && npm run dump-schema -- ../../data",
    "sync-colors": "cd externals/elan && git fetch && git checkout origin/master && cp components/lib/variables/colors.json ../../data",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "type-check": "tsc --noEmit --pretty",
    "compile:server": "gulp compile-server",
    "compile:client": "gulp compile-client",
    "compile": "gulp compile",
    "prepublishOnly": "rm -rf dist && npm run compile",
    "precommit": "lint-staged",
    "prepush": "npm run type-check",
    "prettier": "prettier",
    "prettier-write": "npm run prettier -- --parser typescript --no-semi --trailing-comma es5 --write --print-width 120",
    "prettier-project": "npm run prettier-write -- 'src/**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@storybook/storybook-deployer": "^2.0.0",
    "@types/backbone": "^1.3.33",
    "@types/body-parser": "^1.16.1",
    "@types/cookie-parser": "^1.3.30",
    "@types/csurf": "^1.9.33",
    "@types/enzyme": "^2.7.9",
    "@types/express": "^4.0.35",
    "@types/express-session": "^0.0.32",
    "@types/fetch-mock": "^5.8.2",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^18.1.1",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.7",
    "@types/react-dom": "^15.5.0",
    "@types/react-relay": "^0.9.12",
    "@types/react-test-renderer": "^15.4.4",
    "@types/segment-analytics": "^0.0.27",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.24.0",
    "babel-plugin-react-relay": "^0.10.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-relay-plugin": "https://github.com/alloy/relay/releases/download/v0.9.3/babel-relay-plugin-0.9.3.tgz",
    "babel-template": "^6.2.0",
    "babel-types": "^6.2.0",
    "chokidar": "^1.6.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.2",
    "graphql-config-parser": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-tsc": "^1.3.0",
    "gulp-typescript": "^3.1.6",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "longjohn": "^0.2.12",
    "morgan": "^1.8.1",
    "prettier": "^1.4.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.4.2",
    "relay2ts": "^0.2.0",
    "ts-babel-node": "alloy/ts-babel-node#artsy",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-eslint-rules": "^3.4.0",
    "tslint-react": "^2.4.0",
    "typescript": "^2.3.4",
    "typescript-babel-jest": "^1.0.3",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.2",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-merge": "^4.1.0",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "@storybook/addon-options": "^3.1.2",
    "@storybook/react": "^3.1.3",
    "artsy-passport": "^2.0.3",
    "artsy-xapp": "^1.0.4",
    "babel-polyfill": "^6.23.0",
    "backbone": "^1.3.3",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.1",
    "express": "^4.15.2",
    "history": "^4.6.1",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-relay": "^0.7.4",
    "lodash": "^4.17.4",
    "numeral": "^2.0.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-relay": "https://github.com/alloy/relay/releases/download/v0.9.3/react-relay-0.9.3.tgz",
    "react-sizeme": "^2.3.2",
    "react-styled-flexboxgrid": "^2.0.0",
    "react-url-query": "^1.1.4",
    "request": "^2.81.0",
    "sharify": "^0.1.6",
    "styled-components": "^2.0.1",
    "uuid": "^3.0.1"
  },
  "graphql": {
    "file": "data/schema.json",
    "tsInterfaceName": "RelayProps"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "typescript-babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "tslint --fix",
      "npm run prettier-write --",
      "git add"
    ]
  }
}
