  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "jest": {
    "cacheDirectory": ".cache/jest",
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "babel-jest",
      "\\.graphql$": "jest-raw-loader"
    },
    "testRegex": "/__tests__/.+?\\.test\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/externals/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/*"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup_jest.ts"
    ],
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^luxon$": "<rootDir>/node_modules/luxon",
      "^react$": "<rootDir>/node_modules/react",
      "^Components(.*)$": "<rootDir>/src/Components/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint --fix",
      "yarn prettier-write"
    ],
    "*.json": [
      "yarn prettier-write"
    ]
  },
  "lint-changed": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn prettier -c"
    ],
    "*.json": [
      "yarn prettier -c"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
